{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "commonjs",
    "declaration": true,
    "noImplicitAny": false,
    "removeComments": false,
    "noLib": false,
    "emitDecoratorMetadata": false,
    "experimentalDecorators": true,
    "target": "es2019",
    "sourceMap": false,
    "lib": ["es2019", "dom"],
    "types": ["./typesPatch"],
    "composite": true
  },
  "include": ["modules/**/*"]
}
